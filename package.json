{
  "name": "sigma-fp-decoder",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start:analysis": "ng serve fp-analysis",
    "start:manager": "ng serve fp-manager",
    "build": "ng build fp-analysis && ng build fp-manager",
    "watch": "ng build --watch --configuration development",
