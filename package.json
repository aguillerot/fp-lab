    "prettier": "prettier \"!**/*.{scss,html}\" --check .",
    "prettier:write": "prettier \"!**/*.{scss,html}\" --write ."
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^21.0.0",
    "@angular/common": "^21.0.0",
