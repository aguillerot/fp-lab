    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/router": "^21.0.0",
    "@ngrx/signals": "^21.0.1",
    "@primeuix/themes": "^1.2.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/qrcode": "^1.5.6",
    "jsqr": "^1.4.0",
    "primeicons": "^7.0.0",
    "primeng": "^21.0.2",
    "qrcode": "^1.5.4",
    "rxjs": "~7.8.0",
